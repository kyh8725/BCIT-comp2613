public class B22 {

	private int xxx;
	private int yyy;

	public B22(int xxx, int yyy) {
		this.xxx = xxx;
		this.yyy = yyy;
	};

	/**
	 * @return the xxx
	 */
	public int getXxx() {
		return xxx;
	}

	/**
	 * @param xxx
	 *            the xxx to set
	 */
	public void setXxx(int xxx) {
		this.xxx = xxx;
	}

	/**
	 * @return the yyy
	 */
	public int getYyy() {
		return yyy;
	}

	/**
	 * @param yyy
	 *            the yyy to set
	 */
	public void setYyy(int yyy) {
		this.yyy = yyy;
	}

}
